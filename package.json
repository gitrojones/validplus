{
  "name": "validplus",
  "version": "2.2.0",
  "description": "HTML Inspired Validator, requires light rigging and has graceful fallback",
  "main": "dist/ValidPlus.js",
  "types": "dist/Types/validplus.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "NODE_ENV=development APP=true node --inspect build.js",
    "build": "npm run build:prod",
    "build:prod": "NODE_ENV=production node build.js",
    "build:dev": "NODE_ENV=development node build.js",
    "build:types": "NODE_ENV=production ttsc --emitDeclarationOnly",
    "docs": "typedoc --out docs ./src",
    "test": "npm run test:prod",
    "test:prod": "NODE_ENV=production npm run _test",
    "test:dev": "NODE_ENV=development npm run _test",
    "_test": "TEST=true mocha-webpack -w --webpack-config test.webpack.js --require test/setup.js test/**/*.test.js",
    "test-ci": "PROD=true mocha-webpack --webpack-config test.webpack.js --require test/setup.js test/**/*.test.js",
    "cover": "NODE_ENV=coverage nyc --reporter=lcov --reporter=text npm run test-ci"
  },
  "nyc": {
    "include": [
      "src/**/*.js",
      "src/**/*.ts"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gitrojones/validplus.git"
  },
  "keywords": [
    "validation",
    "html5",
    "forms"
  ],
  "author": "Ryann Jones",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gitrojones/validplus/issues"
  },
  "homepage": "https://github.com/gitrojones/validplus#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@vue/test-utils": "^1.0.3",
    "autoprefixer": "^9.8.5",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "compression-webpack-plugin": "^4.0.0",
    "documentation-loader": "^0.1.1-beta",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdoc": "^3.6.4",
    "jsdoc-vuejs": "^3.0.8",
    "jsdom": "^16.3.0",
    "jsdom-global": "^3.0.2",
    "less": "^3.12.0",
    "less-loader": "^6.2.0",
    "lodash": "^4.17.19",
    "mocha": "^8.0.1",
    "mocha-loader": "^5.1.0",
    "mocha-webpack": "^2.0.0-beta.0",
    "nyc": "^15.1.0",
    "postcss-loader": "^3.0.0",
    "sinon": "^9.0.2",
    "source-map-loader": "^1.0.1",
    "tslint": "^6.1.2",
    "tslint-config-standard": "^9.0.0",
    "ttypescript": "^1.5.10",
    "typedoc": "^0.17.8",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^3.9.6",
    "typescript-transform-paths": "^1.1.14",
    "vue": "^2.6.11",
    "vue-html-webpack-plugin": "^3.2.2",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.0.9",
    "webpack-node-externals": "^2.5.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.4",
    "remove": "^0.1.5"
  }
}
