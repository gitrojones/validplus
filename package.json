{
  "name": "validplus",
  "version": "2.3.0",
  "description": "HTML Inspired Validator, requires light rigging and has graceful fallbacks",
  "main": "dist/ValidPlus.js",
  "types": "dist/Types/validplus.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "NODE_ENV=development APP=true node --inspect build.js",
    "build": "npm run build:prod",
    "build:prod": "NODE_ENV=production node build.js",
    "build:dev": "NODE_ENV=development node build.js",
    "build:types": "NODE_ENV=production ttsc --noUnusedLocals --noUnusedParameters --emitDeclarationOnly",
    "test": "$(npm bin)/jest",
    "docs": "$(npm bin)/jsdoc -c jsdoc.json",
    "lint": "$(npm bin)/eslint . --ext .ts",
    "lint:fix": "npm run list -- --fix"
  },
  "nyc": {
    "include": [
      "src/**/*.js",
      "src/**/*.ts"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gitrojones/validplus.git"
  },
  "keywords": [
    "validation",
    "html5",
    "forms"
  ],
  "_moduleAliases": {
    "@": "src",
    "@dev": "dev",
    "@lib": "lib",
    "@test": "test"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "preset": "ts-jest/presets/js-with-babel",
    "setupFiles": [],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@lib/(.*)$": "<rootDir>/lib/$1",
      "^@test/(.*)$": "<rootDir>/test/$1",
      "^@dev/(.*)$": "<rootDir>/dev/$1",
      "^validplus$": "<rootDir>/validplus.ts"
    },
    "globals": {
      "ts-jest": {
        "compiler": "ttypescript"
      }
    },
    "transform": {
      "^.+\\.vue$": "vue-jest"
    }
  },
  "author": "Ryann Jones",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gitrojones/validplus/issues"
  },
  "homepage": "https://github.com/gitrojones/validplus#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@types/chai": "^4.2.12",
    "@types/chai-arrays": "^1.0.3",
    "@types/chai-as-promised": "^7.1.3",
    "@types/chai-fuzzy": "^1.3.32",
    "@types/chai-string": "^1.4.2",
    "@types/jest": "^26.0.14",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "@vue/test-utils": "^1.1.0",
    "autoprefixer": "^10.0.1",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-const-enum": "^1.0.1",
    "babel-plugin-replace-ts-export-assignment": "0.0.2",
    "better-docs": "^2.3.2",
    "bulma": "^0.9.1",
    "chai": "^4.2.0",
    "chai-arrays": "^2.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-fuzzy": "^1.6.1",
    "chai-string": "^1.5.0",
    "compression-webpack-plugin": "^6.0.2",
    "core-js": "^3.6.5",
    "documentation-loader": "^0.1.1-beta",
    "eslint": "^7.10.0",
    "eslint-plugin-vue": "^7.0.0",
    "eslint-webpack-plugin": "^2.1.0",
    "express": "^4.17.1",
    "file-loader": "^6.1.0",
    "jest": "^26.4.2",
    "jsdoc": "^3.6.6",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "nyc": "^15.1.0",
    "parcel-bundler": "^1.12.4",
    "postcss-loader": "^4.0.3",
    "sinon": "^9.1.0",
    "sinon-chai": "^3.5.0",
    "source-map-loader": "^1.1.0",
    "terser-webpack-plugin": "^4.2.2",
    "ts-jest": "^26.4.1",
    "tslint-config-standard": "^9.0.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.0.3",
    "typescript-transform-paths": "^2.0.1",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-html-webpack-plugin": "^3.2.2",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.3",
    "vue-property-decorator": "^9.0.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2"
  }
}
