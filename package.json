{
  "name": "validplus",
  "version": "1.4.1",
  "description": "HTML Inspired Validator, requires light rigging and has graceful fallback",
  "main": "dist/ValidPlus.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "NODE_ENV=development APP=true node --inspect build.js",
    "build": "npm run build:prod",
    "build:prod": "NODE_ENV=production node build.js",
    "build:dev": "NODE_ENV=development node build.js",
    "test": "npm run test:prod",
    "test:prod": "NODE_ENV=production npm run _test",
    "test:dev": "NODE_ENV=development npm run _test",
    "_test": "TEST=true mocha-webpack -w --webpack-config test.webpack.js --require test/setup.js test/**/*.test.js",
    "test-ci": "PROD=true mocha-webpack --webpack-config test.webpack.js --require test/setup.js test/**/*.test.js",
    "cover": "NODE_ENV=coverage nyc --reporter=lcov --reporter=text npm run test-ci"
  },
  "nyc": {
    "include": [
      "src/**/*.js",
      "src/**/*.ts"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gitrojones/validplus.git"
  },
  "keywords": [
    "validation",
    "html5",
    "forms"
  ],
  "author": "Ryann Jones",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gitrojones/validplus/issues"
  },
  "homepage": "https://github.com/gitrojones/validplus#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-typescript": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-typescript": "^7.1.0",
    "@vue/test-utils": "^1.0.0-beta.25",
    "autoprefixer": "^9.3.1",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "documentation-loader": "^0.1.1-beta",
    "express": "^4.16.4",
    "file-loader": "^1.1.11",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdoc": "^3.5.5",
    "jsdoc-vuejs": "^2.3.0",
    "jsdom": "^11.12.0",
    "jsdom-global": "^3.0.2",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mocha": "^5.2.0",
    "mocha-loader": "^1.1.3",
    "mocha-webpack": "^2.0.0-beta.0",
    "nyc": "^13.1.0",
    "postcss-loader": "^3.0.0",
    "sinon": "^6.3.5",
    "source-map-loader": "^0.2.4",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.13.0",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^3.2.1",
    "vue": "^2.5.17",
    "vue-html-webpack-plugin": "^3.2.2",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.25.1",
    "webpack-cli": "^2.1.5",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  }
}
